/** Author: eye8*/package org.flowplayer.controls.controllers {    	import org.flowplayer.controls.Controlbar;	import flash.external.ExternalInterface;	import org.flowplayer.controls.SkinClasses;		import org.flowplayer.ui.controllers.AbstractButtonController;	import org.flowplayer.ui.buttons.ButtonEvent;	import org.flowplayer.view.Flowplayer;	import flash.display.DisplayObjectContainer;	import flash.geom.ColorTransform;		import flash.net.URLRequest;	import flash.net.navigateToURL;		import org.flowplayer.ui.buttons.ConfigurableWidget;		public class VialoguesButtonController extends AbstractButtonController {		private static var defaultColorTransform:ColorTransform;				public function VialoguesButtonController() {			super();			defaultColorTransform = new ColorTransform();//ca2027			defaultColorTransform.color = 0xca2027;		}				override public function get view():ConfigurableWidget {			//_decoratedView.transform.colorTransform = defaultColorTransform;			return _decoratedView;		}				override public function get name():String {			return "vialogues";		}				override public function get defaults():Object {			return {				tooltipEnabled: false,				tooltipLabel: "Visit Vialogues.Com",				visible: true,				enabled: true			};		}		override protected function get faceClass():Class {			log.debug("returning vialogues face class");			return SkinClasses.getClass("fp.VialoguesButton");		}				override protected function onButtonClicked(event:ButtonEvent):void {			navigateToURL(new URLRequest("https://vialogues.com"), "_blank");		}	}}